{
  "name": "@zephyr/db",
  "description": "Zephyr's database & cache package",
  "version": "2.0.37-dev",
  "license": "AGPL-3.0",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    }
  },
  "scripts": {
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:push": "prisma db push --skip-generate",
    "backup": "tsx scripts/backup.ts",
    "migrate:safe": "pnpm run backup && pnpm run pre-migrate && pnpm prisma migrate dev --create-only",
    "migrate:apply": "pnpm prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@zephyr/config": "workspace:*",
    "date-fns": "^4.1.0",
    "ioredis": "^5.8.0",
    "jose": "^6.1.0",
    "meilisearch": "^0.53.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "prisma": "^6.16.3",
    "typescript": "^5.9.3"
  }
}
